    </validationRules>
    <validationRules>
        <fullName>Skills_Updation_NotAllowed</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(PRIORVALUE( Skills__c),&apos;Java&apos;),
ISPICKVAL(PRIORVALUE( Skills__c),&apos;C++&apos;)),
ISPICKVAL( Skills__c , &apos;C&apos;) ,
NOT(ISNEW())
)</errorConditionFormula>
        <errorMessage>Cannot Change skills to C.</errorMessage>
    </validationRules>
