    <validationRules>
        <fullName>Valid_SSN_Format</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( OR( LEN( Social_Security_Number__c )=0, REGEX( Social_Security_Number__c ,&quot;[0-9]{3}-[0-9]{2}-[0-9]{4}&quot;)  ) )</errorConditionFormula>
        <errorDisplayField>Social_Security_Number__c</errorDisplayField>
        <errorMessage>enter proper format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_Code_Consistent_With_State</fullName>
        <active>true</active>
        <errorConditionFormula>AND(VLOOKUP( $ObjectType.Zip_Code__c.Fields.State__c , $ObjectType.Zip_Code__c.Fields.Name,  LEFT( Zip_Postal_Code__c ,5) ) &lt;&gt;  Country__c )</errorConditionFormula>
        <errorDisplayField>Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>zip code does not exist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_Code_must_be_Valid_Postal_US_Code</fullName>
        <active>true</active>
        <description>5 or 5-4 format</description>
        <errorConditionFormula>( Country__c  == &quot;US&quot;  ||  Country__c  == &quot;USA&quot; )  &amp;&amp;  NOT( REGEX( Zip_Postal_Code__c ,&quot;\\d{5}(-\\d{4})?&quot;) )</errorConditionFormula>
        <errorDisplayField>Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>5 or 5-4 format</errorMessage>
    </validationRules>
